import re 
import urllib2
from bs4 import BeautifulSoup
import os,sys

         
counter = 0
page = urllib2.urlopen("http://www.craigslist.org/about/sites").read()
soup = BeautifulSoup(page)
f = open(sites.py),"a")
counter+=1
for x in range (42,100):
    try:
      data = "(\""+str((soup.findAll("option")[x].contents.pop()))+"\",\""+str(soup.findAll("option")[x].attrs.values().pop())+'\"),\n'
    except IndexError:
      break

    f.write(data)
f.close()
